{
  "version": 1,
  "projects": {
    "next-tutorial": {
      "root": "apps/next-tutorial",
      "sourceRoot": "apps/next-tutorial",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/next-tutorial",
            "outputPath": "dist/apps/next-tutorial"
          }
        },
        "serve": {
          "builder": "@nrwl/next:dev-server",
          "options": {
            "buildTarget": "next-tutorial:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "next-tutorial:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/next-tutorial/.eslintrc",
            "tsConfig": [
              "apps/next-tutorial/tsconfig.json",
              "apps/next-tutorial/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/next-tutorial/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/next-tutorial/jest.config.js",
            "tsConfig": "apps/next-tutorial/tsconfig.spec.json"
          }
        }
      }
    },
    "next-tutorial-e2e": {
      "root": "apps/next-tutorial-e2e",
      "sourceRoot": "apps/next-tutorial-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/next-tutorial-e2e/cypress.json",
            "tsConfig": "apps/next-tutorial-e2e/tsconfig.e2e.json",
            "devServerTarget": "next-tutorial:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "next-tutorial:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/next-tutorial-e2e/.eslintrc",
            "tsConfig": ["apps/next-tutorial-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/next-tutorial-e2e/**"]
          }
        }
      }
    },
    "react-app": {
      "root": "apps/react-app",
      "sourceRoot": "apps/react-app/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/react-app",
            "index": "apps/react-app/src/index.html",
            "main": "apps/react-app/src/main.tsx",
            "polyfills": "apps/react-app/src/polyfills.ts",
            "tsConfig": "apps/react-app/tsconfig.app.json",
            "assets": [
              "apps/react-app/src/favicon.ico",
              "apps/react-app/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/babel"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react-app/src/environments/environment.ts",
                  "with": "apps/react-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "react-app:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/react-app/.eslintrc",
            "tsConfig": [
              "apps/react-app/tsconfig.app.json",
              "apps/react-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/react-app/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/react-app/jest.config.js",
            "tsConfig": "apps/react-app/tsconfig.spec.json"
          }
        }
      }
    },
    "react-app-e2e": {
      "root": "apps/react-app-e2e",
      "sourceRoot": "apps/react-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-app-e2e/cypress.json",
            "tsConfig": "apps/react-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "react-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/react-app-e2e/.eslintrc",
            "tsConfig": ["apps/react-app-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/react-app-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "next-tutorial"
}
